package tw.hightwo.tutor;

public class Hightwo99 {

	public static void main(String[] args) {
		for (int j = 1; j <= 9; j++) {
			for (int i = 2; i <= 5; i++) {
				int result = i * j;
				System.out.printf("%d x %d = %d	", i, j, result);
			}
			System.out.println();
		}
		System.out.println("------------------------------------------------------");
		for (int j = 1; j <= 9; j++) {
			for (int i = 6; i <= 9; i++) {
				int result = i * j;
				System.out.printf("%d x %d = %d	", i, j, result);
			}
			System.out.println();
		}
	}
}
